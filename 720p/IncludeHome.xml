	<include name="Sublist">
		<posy>395</posy>
		<width>250</width>
		<height>275</height>
		<scrolltime>200</scrolltime>
		<itemlayout height="40" width="250">
			<control type="label">
				<posx>125</posx>
				<posy>0</posy>
				<width>240</width>
				<height>40</height>
				<textoffsetx>0</textoffsetx>
				<font>Submenu</font>
				<info>ListItem.Label</info>
				<align>center</align>
				<aligny>center</aligny>
				<scroll>false</scroll>
			</control>
		</itemlayout>
		<focusedlayout height="40" width="250">
