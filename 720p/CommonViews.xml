	<include name="Thumbs">
		<control type="group" description="Non Music Thumbs">
			<visible>!ControlGroup(9000).HasFocus() + !stringcompare(Container.Listitem(0).Label,..)</visible>
			<visible>![Window.IsActive(MyMusicNav.xml) | Window.IsActive(MyMusicSongs.xml) | Window.IsActive(addonbrowser.xml)]</visible>
			<posx>130</posx>
			<posy>46</posy>
			<aligny>center</aligny>
			<control type="image">
				<visible>Container.Content(Movies) | Container.Content(Seasons) | [Container.Content(TVShows) + Skin.HasSetting(TVPoster)]</visible>
				<posx>-170</posx>
				<posy>420</posy>
				<width>720</width>
				<height>256</height>
				<aspectratio align="center" aligny="bottom">stretch</aspectratio>
				<fadetime>200</fadetime>
				<texture background="true">img/IconStage.png</texture>
			</control>
			<control type="image">
				<width>396</width>
				<height>614</height>
				<aspectratio align="left" aligny="bottom">keep</aspectratio>
				<fadetime>200</fadetime>
				<texture border="1" diffuse="img/Diffuse.png" background="true">$INFO[ListItem.Icon]</texture>
				<animation condition="true" effect="rotatey" start="0" end="-7" center="260," time="0" reversible="false">Conditional</animation>
			</control>
			<control type="image" description="Reflection">
				<posx>1</posx>
				<posy>612</posy>
				<width>394</width>
				<height>80</height>
				<bordersize>1</bordersize>
				<aspectratio align="left" aligny="top">scale</aspectratio>
				<fadetime>200</fadetime>
				<texture background="true" diffuse="img/Reflection2.png" flipy="true">$INFO[ListItem.Icon]</texture>
				<animation condition="true" effect="rotatey" start="0" end="-7" center="260," time="0" reversible="false">Conditional</animation>
			</control>
		</control>
	</include>	
