						<posx>15</posx>
						<posy>5</posy>
						<width>120</width>
						<height>80</height>
						<bordertexture border="1">img/ButtonFO.png</bordertexture>
						<bordersize>1</bordersize>
						<texture fallback="img/BlackDot.png" background="true">$INFO[ListItem.Icon]</texture>
						<aspectratio>scale</aspectratio>
						<fadetime>0</fadetime>
					</control>
					<control type="label">
						<posy>20</posy>
						<posx>75</posx>
